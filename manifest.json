{
	"manifest_version": 2,
	"name": "Keep My Monitors Awake",
	"version": "1.0",
	"description": "A simple extension to prevent your monitors from going to sleep.",
	"author": "Franck Dupont",
	"icons": {
		"16": "awake-logo-on16.png",
      	"32": "awake-logo-on32.png",
      	"48": "awake-logo-on48.png",
      	"128": "awake-logo-on128.png"
	},
	"background": {
	    "scripts": ["background.js"],
	    "persistent": true
	},
	"browser_action": {
    "default_icon": {
    	"16": "awake-logo-on16.png",
      	"32": "awake-logo-on32.png",
      	"48": "awake-logo-on48.png",
      	"128": "awake-logo-on128.png"
    },
    "default_title": "Keep My Monitors Awake"
  }
}
